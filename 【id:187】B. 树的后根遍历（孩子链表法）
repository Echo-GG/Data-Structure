#include <iostream>
#include <vector>
using namespace std;

void dfs_postorder(int u, const vector<vector<int>>& children, const vector<char>& val) 
{
    for (int v : children[u])
        dfs_postorder(v, children, val);
    cout << val[u];
}

int main() 
{
    int n, r;
    cin >> n >> r;
    vector<char> val(n);
    vector<vector<int>> children(n);

    for (int i = 0; i < n; i++) 
	{
        char c;
        cin >> c;
        val[i] = c;
        int tmp;
        while (cin >> tmp && tmp != -1) 
		{
            children[i].push_back(tmp);
        }
    }

    dfs_postorder(r, children, val);
    cout << endl;
    return 0;
}
